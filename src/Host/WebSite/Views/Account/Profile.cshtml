<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="~/">Home</a></li>
        <li class="breadcrumb-item"><a>User</a></li>
        <li class="breadcrumb-item active" aria-current="page">Profile</li>
    </ol>
</nav>


<div id="app">
    <form onsubmit="return profileVM.save();">
        <div class="form-group">
            <label for="Email">Email</label>
            <input id="Email" type="text" class="form-control" v-model="user.email" placeholder="Email" maxlength="50" required>
        </div>
        <div class="form-group">
            <label for="Phone">Phone</label>
            <input id="Phone" type="text" class="form-control" v-model="user.phone" placeholder="Phone" maxlength="50" required>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary float-right">Save</button>
        </div>
    </form>
</div>

@section Scripts{
<partial name="Script" model='"account/profile"' />
}